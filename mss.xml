    <mkdir dir="${artifact.temp.output.mss:war}"/>
    <zip destfile="${temp.jar.path.Mss.war}">
      <zipfileset dir="${artifact.output.mss:war_exploded}"/>
    </zip>
    <copy file="${temp.jar.path.Mss.war}" tofile="${artifact.temp.output.mss:war}/Mss.war"/>
  </target>
  
